{{#sprites}}
.{{name}} {
  display: inline-block;
  overflow: hidden;
  position: relative;  
  background-image: url({{{escaped_image}}});
  background-position: {{px.offset_x}} {{px.offset_y}};
  width: {{px.width}};
  height: {{px.height}};
  margin-right: 5px;
  top: 3px;
  cursor: pointer;
}
{{/sprites}}
